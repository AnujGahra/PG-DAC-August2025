package com.mainapp;

public class Account {
	
	
	private int an;
    private String bankName;

    public void setAn(int an) {
        this.an = an;
    }

    public void setBankName(String bankName) {
        this.bankName = bankName;
    }

    @Override
    public String toString() {
        return an + "-" + bankName;
    }
}
