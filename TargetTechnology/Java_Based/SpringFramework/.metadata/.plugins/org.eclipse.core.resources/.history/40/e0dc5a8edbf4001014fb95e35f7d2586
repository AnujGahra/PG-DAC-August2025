package com.beans;

public class Employee {
	
	private int x = 10;
	
	static {
		System.out.println("EMPLOYEE BEAN LOADING...");
	}
	
	public Employee() {
		System.out.println("EMPLOYEE BEAN INSTANTIATION...");
	}
	
	public void test() {
		x++;
		System.out.println("EMPLOYEE BEAN USING..." + x);
	}

}
