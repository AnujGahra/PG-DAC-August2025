package transfile.Data;

import java.io.FileInputStream;
import java.io.FileNotFoundException;

public class Launch {

	public static void main(String[] args) throws Exception {
		
		
		FileInputStream fis = new FileInputStream("/Users/anujgahra/Developer/PG-DAC/Java_module/Resources/f1/abc.txt");
		String data = "";
		String vowels="";
		
		int temp = 0;
		while(true) {
			temp = fis.read();
			if(temp == -1) break;
			data = data+(char)temp;
		}
		System.out.println(data);
		
		String[] split = data.split(" ");
		for(int i=0; i<split.length; i++) {
			if(split[i].startsWith("a") || split[i].startsWith("e") || split[i].startsWith("i") || split[i].startsWith("o") || split[i].startsWith("u")) {
				vowels = vowels+split[i] + " ";
			}
		}
		System.out.println(vowels);

	}

}
