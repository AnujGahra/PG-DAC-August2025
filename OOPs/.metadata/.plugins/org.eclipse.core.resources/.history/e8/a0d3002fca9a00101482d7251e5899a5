package join.method;

public class Database1 extends Thread {
	
	public String collect[] = new String[4];
	
	@Override
	public void run() {
		String data[] = {"d1", "d2", "d3", "d4"};
		int i=0;
		
		for(String d: data) {
			collect[i] = d;
			System.out.println("Data" + i + "Collected");
			i++;
		}
	}

}
